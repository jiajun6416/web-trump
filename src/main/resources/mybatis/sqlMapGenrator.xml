<generatorConfiguration>
	<!-- 制定数据库驱动包的路径 千万别放中文路径下 -->
	<classPathEntry location="D:\maven\repository\mysql-connector-java-5.1.28-bin.jar" />
	<!-- 配置数据源和生成的代码所存放的位置 -->
	<context id="context1">
		<commentGenerator>
